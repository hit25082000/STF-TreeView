{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color",
          "-ms-touch-action",
          "user-select",
          "-webkit-text-size-adjust",
          "-ms-filter",
          "-moz-appearance"
        ]
      }
    ],
    "css-prefix-order": "off"
  }
}